{
  "manifest_version": 3,
  "name": "表格內容編輯器",
  "version": "1.0",
  "description": "允許編輯網頁表格內容的 Chrome 擴充功能",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
} 